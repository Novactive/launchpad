{
    "algorithm": "OPENSSL",
    "alias": "ez.phar",
    "banner": "eZ Launchpad",
    "chmod": "0755",
    "compression": "GZ",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": [
        "src",
        "config",
        "payload"
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "tests",
                "Tests"
            ],
            "in": "vendor"
        }
    ],
    "git-tag": "package_version",
    "main": "bin/ez-prod",
    "key": "ezlaunchpad-private.pem",
    "output": "ez.phar",
    "stub": true
}
